{
  "name": "electrongular",
  "productName": "Electrongular",
  "version": "1.0.0",
  "description": "A sample desktop application that combines Electron and Angular. ",
  "main": "src/index.ts",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "tslint --project . --format verbose"
  },
  "keywords": [
    "angular",
    "electron",
    "angular-material",
    "tslint",
    "typescript"
  ],
  "author": "György Balássy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/balassy/electrongular.git"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm"
      },
      "electronWinstallerConfig": {
        "name": "devcon"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "devcon"
      }
    }
  },
  "dependencies": {
    "@angular/animations": "^5.0.1",
    "@angular/cdk": "^5.0.0-rc0",
    "@angular/common": "^5.0.1",
    "@angular/compiler": "^5.0.1",
    "@angular/core": "^5.0.1",
    "@angular/forms": "^5.0.1",
    "@angular/material": "^5.0.0-rc0",
    "@angular/platform-browser": "^5.0.1",
    "@angular/platform-browser-dynamic": "^5.0.1",
    "@angular/router": "^5.0.1",
    "electron": "^1.7.9",
    "electron-compile": "^6.4.2",
    "electron-devtools-installer": "^2.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "material-design-icons": "^3.0.1",
    "reflect-metadata": "^0.1.9",
    "rxjs": "^5.5.2",
    "tslib": "^1.4.0",
    "typeface-roboto": "^0.0.45",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^4.1.3",
    "electron-prebuilt-compile": "1.7.9",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1"
  }
}
